{
  "name": "webspy",
  "private": true,
  "version": "0.1.3",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:windows": "tsc && vite build && electron-builder  --win --x64 --ia32 -p always",
    "build:intel": "tsc && vite build && npx electron-builder",
    "make": "tsc && vite build && electron-builder --mac --x64 --arm64",
    "makeAll": "tsc && vite build && electron-builder -mwl"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "axios": "^1.4.0",
    "clientjs": "^0.2.1",
    "crypto-js": "^4.1.1",
    "electron-store": "^8.1.0",
    "mem": "^9.0.2",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "electron-updater": "^6.1.1",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.10.1",
    "react-router-dom": "^6.14.2",
    "tailwind": "^4.0.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/clientjs": "^0.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "electron": "^24.6.5",
    "electron-builder": "^23.6.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "build": {
    "publish": {
      "provider": "github",
      "owner": "NandoAtr",
      "repo": "electron-app-autoupdater"
    },
    "appId": "com.SUPPLIER.APP_NAME",
    "win": {
      "icon": "build/favicon.png",
      "target": "nsis",
      "publish": [
        {
          "provider": "github",
          "owner": "NandoAtr",
          "repo": "electron-app-autoupdater-windows"
        }
      ]
    },
    "mac": {
      "icon": "build/favicon.png",
      "category": "public.app-category.utilities",
      "target": "dmg",
      "publish": [
        {
          "provider": "github",
          "owner": "NandoAtr",
          "repo": "electron-app-autoupdater"
        }
      ]
    },
    "extraResources": [
      "res",
      "./build/favicon.png"
    ],
    "directories": {
      "buildResources": "build",
      "output": "Webspy--macos-0.1.1"
    }
  },
  "main": "dist-electron/main.js"
}
